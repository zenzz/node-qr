#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/matt/Downloads/qr-node-api/node_modules/.pnpm/qrcode@1.5.1/node_modules/qrcode/bin/node_modules:/Users/matt/Downloads/qr-node-api/node_modules/.pnpm/qrcode@1.5.1/node_modules/qrcode/node_modules:/Users/matt/Downloads/qr-node-api/node_modules/.pnpm/qrcode@1.5.1/node_modules:/Users/matt/Downloads/qr-node-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/matt/Downloads/qr-node-api/node_modules/.pnpm/qrcode@1.5.1/node_modules/qrcode/bin/node_modules:/Users/matt/Downloads/qr-node-api/node_modules/.pnpm/qrcode@1.5.1/node_modules/qrcode/node_modules:/Users/matt/Downloads/qr-node-api/node_modules/.pnpm/qrcode@1.5.1/node_modules:/Users/matt/Downloads/qr-node-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../qrcode/bin/qrcode" "$@"
else
  exec node  "$basedir/../qrcode/bin/qrcode" "$@"
fi
